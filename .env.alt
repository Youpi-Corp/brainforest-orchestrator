NODE_ENV=production
LOG_LEVEL=info
FRONTEND_API_URL=https://test.api.brain-forest.works:8443
FRONTEND_PORT=8080
FRONTEND_SSL_PORT=8443
BACKEND_PORT=3000
JWT_SECRET=your-super-secure-jwt-secret-change-this-in-production
CORS_ORIGINS=https://test.brain-forest.works:8443,https://www.test.brain-forest.works:8443
DATABASE_URL=postgresql://brainforest_user:secure_db_password@db:5432/brainforest_prod
POSTGRES_DB=brainforest_prod
POSTGRES_USER=brainforest_user
POSTGRES_PASSWORD=secure_db_password
REDIS_PASSWORD=secure_redis_password
REDIS_URL=redis://:secure_redis_password@redis:6379
SSL_CERT_PATH=/etc/letsencrypt/live/test.brain-forest.works/fullchain.pem
SSL_KEY_PATH=/etc/letsencrypt/live/test.brain-forest.works/privkey.pem
SSL_EMAIL=admin@brain-forest.works
DOMAIN_NAME=test.brain-forest.works
API_DOMAIN=test.api.brain-forest.works
ENABLE_MONITORING=true
BACKUP_ENABLED=true
BACKUP_SCHEDULE=0 2 * * *
BACKUP_RETENTION_DAYS=30
BACKEND_REPLICAS=1
WORKER_PROCESSES=1
